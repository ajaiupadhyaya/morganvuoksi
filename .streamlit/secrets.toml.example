# MorganVuoksi Terminal Secrets Configuration
# Rename this file to secrets.toml for production deployment

[api_keys]
# Financial Data APIs (Optional - yfinance is free)
alpha_vantage_key = "your_alpha_vantage_key_here"
quandl_key = "your_quandl_key_here"
iex_cloud_key = "your_iex_cloud_key_here"

# AI/ML APIs (Optional - for advanced features)
openai_api_key = "your_openai_key_here"

# Database Configuration (Optional)
[database]
connection_string = "your_database_connection_here"

# External Services (Optional)
[services]
redis_url = "your_redis_url_here"